{
  "name": "skyhelper",
  "version": "5.0.0",
  "description": "A Bot for Sky: Children of the Light",
  "main": "src/index.ts",
  "type": "module",
  "keywords": [
    "SkyCoTL",
    "SkyShards",
    "SkyDiscordBot",
    "Sky: Children of the Light"
  ],
  "scripts": {
    "start": "bun run src/index.ts",
    "build": "tsc -p tsconfig.json",
    "dev": "bun run src/index.ts",
    "commands": "bun run src/libs/registerCommands.ts",
    "bun-cmd": "bun run src/libs/registerCommands.ts",
    "format": "npx prettier --write \"**/*.{js,css,ejs, ts}\""
  },
  "author": "Naiyar Imam",
  "license": "MIT",
  "dependencies": {
    "@sapphire/stopwatch": "^1.5.2",
    "@sapphire/type": "^2.4.4",
    "chalk": "^4.1.2",
    "cropify": "^1.0.9",
    "discord.js": "^14.12.1",
    "dotenv": "^16.3.1",
    "fixedsize-map": "^1.0.1",
    "moment": "^2.29.4",
    "moment-duration-format": "^2.3.2",
    "moment-timezone": "^0.5.43",
    "mongoose": "^7.5.0",
    "node-cron": "^3.0.3",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "skyhelper-utils": "^1.2.5",
    "table": "^6.8.1",
    "topgg-autoposter": "^2.0.2",
    "uuid": "^9.0.1"
  },
  "imports": {
    "#handlers": [
      "./dist/handlers/index.js"
    ],
    "#handlers/*": [
      "./dist/handlers/*.js"
    ],
    "#src/*": [
      "./dist/*.js"
    ],
    "#commands/*": [
      "./dist/commands/*.js"
    ],
    "#root/*": [
      "./*"
    ],
    "#schemas/*": [
      "./dist/database/schemas/*.js"
    ],
    "#structures/*": [
      "./dist/structures/*.js"
    ],
    "#structures": [
      "./dist/structures/index.js"
    ],
    "#libs/*": [
      "./dist/libs/*.js"
    ],
    "#libs": [
      "./dist/libs/index.js"
    ],
    "#functions/*": [
      "./dist/functions/*.js"
    ],
    "#functions": [
      "./dist/functions/index.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.6",
    "@types/moment-duration-format": "^2.2.6",
    "@types/node": "^20.12.5",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^46.8.2",
    "prettier": "^3.0.3",
    "prettier-plugin-ejs": "^0.0.18",
    "typescript": "^5.4.4"
  },
  "trustedDependencies": [
    "@sapphire/type"
  ]
}
