{
  "name": "skyhelper",
  "version": "5.1.0",
  "description": "A Bot for Sky: Children of the Light",
  "main": "dist/src/index.js",
  "type": "module",
  "keywords": [
    "SkyCoTL",
    "SkyShards",
    "SkyDiscordBot",
    "Sky: Children of the Light"
  ],
  "scripts": {
    "start": "npm run build && node dist/src/index.js",
    "build": "tsc -p tsconfig.json",
    "dev": "bun run src/index.ts",
    "commands": "node dist/src/libs/registerCommands.js",
    "bun-cmd": "bun run src/libs/registerCommands.ts",
    "format": "npx prettier --write \"**/*.{js,css,ejs, ts}\""
  },
  "author": "Naiyar Imam",
  "license": "MIT",
  "dependencies": {
    "@napi-rs/canvas": "^0.1.52",
    "@nestjs/common": "^10.3.8",
    "@nestjs/core": "^10.3.9",
    "@nestjs/platform-express": "^10.3.8",
    "@sapphire/stopwatch": "^1.5.2",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "cropify": "^1.0.9",
    "discord.js": "^14.12.1",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "fixedsize-map": "^1.0.1",
    "moment": "^2.29.4",
    "moment-duration-format": "^2.3.2",
    "moment-timezone": "^0.5.43",
    "mongoose": "^7.5.0",
    "node-cron": "^3.0.3",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "skyhelper-utils": "^1.2.5",
    "table": "^6.8.1",
    "topgg-autoposter": "^2.0.2",
    "uuid": "^9.0.1"
  },
  "imports": {
    "#handlers": [
      "./dist/src/handlers/index.js"
    ],
    "#handlers/*": [
      "./dist/src/handlers/*.js"
    ],
    "#src/*": [
      "./dist/src/*.js"
    ],
    "#commands/*": [
      "./dist/src/commands/*.js"
    ],
    "#root/*": [
      "./*"
    ],
    "#schemas/*": [
      "./dist/src/database/schemas/*.js"
    ],
    "#structures/*": [
      "./dist/src/structures/*.js"
    ],
    "#structures": [
      "./dist/src/structures/index.js"
    ],
    "#libs/*": [
      "./dist/src/libs/*.js"
    ],
    "#libs": [
      "./dist/src/libs/index.js"
    ]
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.6",
    "@types/moment-duration-format": "^2.2.6",
    "@types/node": "^20.12.5",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^46.8.2",
    "prettier": "^3.0.3",
    "prettier-plugin-ejs": "^0.0.18",
    "source-map-support": "^0.5.21",
    "typescript": "^5.4.4"
  },
  "trustedDependencies": [
    "@nestjs/core",
    "@sapphire/type"
  ]
}

